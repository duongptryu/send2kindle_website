state.signIn;\n    if (name === \"username\") {\n      old_info.username = value;\n    } else {\n      old_info.password = value;\n    }\n    this.setState({\n      SignIn: {\n        ...old_info,\n      },\n    });\n  }\n\n  async signInHandle(e) {\n    e.preventDefault();\n    const info = this.state.signIn;\n    if (info.username.length === 0 || info.password.length === 0) {\n      alert(\"Please complete input before submit\");\n      return false;\n    }\n    let status = \"\";\n    const data = this.state.signIn;\n    let result = await fetch(\"http://localhost:8000/api/token\", {\n      method: \"POST\",\n      mode: \"cors\",\n      credentials: \"include\",\n      headers: {\n        \"Content-Type\": \"application/json\",\n      },\n      body: JSON.stringify(data),\n    })\n      .then((res) => {\n        status = res.status\n        return res.json() \n      })\n      .catch((err) => {\n        status = err.status\n        return err.json() \n      });\n    if(status === 200){\n      alert(\"Login success\")\n      localStorage.setItem('token', result['access_token'])\n      window.location.href = \"/\"\n    }else{\n      console.log(result)\n      this.setState({\n        error:result['detail']\n      })\n    }\n  }\n\n  async signUpHandle(e) {\n    console.log(this.state.signUp)\n    e.preventDefault();\n    const info = this.state.signUp;\n    if (info.username.length === 0 || info.password.length === 0 || info.email.length === 0 || info.first_name.length === 0 || info.last_name.length === 0) {\n      alert(\"Please complete input before submit\");\n      return false;\n    }\n    let status = \"\";\n    const data = this.state.signUp;\n    let result = await fetch(\"http://localhost:8000/api/create-user\", {\n      method: \"POST\",\n      mode: \"cors\",\n      credentials: \"include\",\n      headers: {\n        \"Content-Type\": \"application/json\",\n      },\n      body: JSON.stringify(data),\n    })\n      .then((res) => {\n        status = res.status\n        return res.json() \n      })\n      .catch((err) => {\n        status = err.status\n        return err.json() \n      });\n    if(status === 201){\n      alert(result['detail'])\n      window.location.href = \"/sign-in\"\n    }else{\n      console.log(result)\n      this.setState({\n        error:result['detail']\n      })\n    }\n  }\n\n  render() {\n    \n    return (\n      <Router>\n        <div className=\"main\">\n          <Container>\n            <div className=\"main-2\">\n              <NavBar />\n              <Switch>\n                <Route path=\"/sign-in\">\n                  <SignIn\n                    signInClick={this.signInHandle}\n                    onchange={this.signInOnchange}\n                    error = {this.state.error}\n                  />\n                </Route>\n                <Route path=\"/guide/\">\n                  <Guide />\n                </Route>\n                <Route path=\"/sign-up\">\n                  <SignUp\n                    signUpClick={this.signUpHandle}\n                    onchange={this.signUpOnchange}\n                    error={this.state.error}\n                  />\n                </Route>\n                <Route path=\"/\">\n                  <Main />\n                </Route>\n              </Switch>\n              <Footer />\n            </div>\n          </Container>\n        </div>\n      </Router>\n    );\n  }\n}\nexport default App;\n","C:\\Users\\Ryu\\Desktop\\project_sublist3r_website\\sublist3r\\src\\reportWebVitals.js",[],"C:\\Users\\Ryu\\Desktop\\project_sublist3r_website\\sublist3r\\src\\components\\NavBar.js",[],"C:\\Users\\Ryu\\Desktop\\project_sublist3r_website\\sublist3r\\src\\components\\Footer.js",[],"C:\\Users\\Ryu\\Desktop\\project_sublist3r_website\\sublist3r\\src\\components\\Main.js",[],"C:\\Users\\Ryu\\Desktop\\project_sublist3r_website\\sublist3r\\src\\components\\Guide.js",[],"C:\\Users\\Ryu\\Desktop\\project_sublist3r_website\\sublist3r\\src\\components\\Op